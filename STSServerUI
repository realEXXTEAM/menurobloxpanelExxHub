print("hello there script not loaded, anyways in proggres in my project")

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Hapus UI lama
if playerGui:FindFirstChild("ToolsUI") then
	playerGui.ToolsUI:Destroy()
end

-- ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "STSServerUI"
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui
screenGui.Enabled = false

-- Frame utama
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 320, 0, 100)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.AnchorPoint = Vector2.new(0.5, 0.5)
frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
frame.BackgroundTransparency = 0.25
frame.BorderSizePixel = 0
frame.Parent = screenGui
frame.ZIndex = 10

-- Tombol close “X”
local closeBtn = Instance.new("TextButton")
closeBtn.Parent = frame
closeBtn.Size = UDim2.new(0, 25, 0, 25)
closeBtn.Position = UDim2.new(1, -30, 0, 5)
closeBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
closeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
closeBtn.Text = "X"
closeBtn.Font = Enum.Font.GothamBold
closeBtn.TextSize = 18
closeBtn.BorderSizePixel = 0
closeBtn.ZIndex = 11
closeBtn.AutoButtonColor = true

-- Gambar profil
local image = Instance.new("ImageLabel")
image.Parent = frame
image.Size = UDim2.new(0, 80, 0, 80)
image.Position = UDim2.new(0, 15, 0.5, -40)
image.BackgroundTransparency = 1
image.Image = "rbxassetid://6031071053"
image.ZIndex = 11

-- Nama “Error”
local nameLabel = Instance.new("TextLabel")
nameLabel.Parent = frame
nameLabel.Position = UDim2.new(0, 110, 0, 20)
nameLabel.Size = UDim2.new(1, -150, 0, 25)
nameLabel.BackgroundTransparency = 1
nameLabel.Font = Enum.Font.GothamBold
nameLabel.TextColor3 = Color3.fromRGB(255, 80, 80)
nameLabel.TextSize = 22
nameLabel.ZIndex = 11
nameLabel.TextXAlignment = Enum.TextXAlignment.Left
nameLabel.Text = "Error"

-- Pesan
local msg = Instance.new("TextLabel")
msg.Parent = frame
msg.Position = UDim2.new(0, 110, 0, 50)
msg.Size = UDim2.new(1, -120, 0, 25)
msg.BackgroundTransparency = 1
msg.Font = Enum.Font.Gotham
msg.TextColor3 = Color3.fromRGB(200, 200, 200)
msg.TextSize = 18
msg.Text = "Coming Soon"
msg.ZIndex = 11
msg.TextXAlignment = Enum.TextXAlignment.Left

-- Tombol close manual
closeBtn.MouseButton1Click:Connect(function()
	screenGui.Enabled = false
end)

-- Always auto-disable tiap 5 detik (loop)
task.spawn(function()
	while task.wait(5) do
		if screenGui.Enabled then
			screenGui.Enabled = false
		end
	end
end)
